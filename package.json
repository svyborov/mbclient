{
  "name": "mbclient",
  "version": "2.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Message broker library",
  "keywords": [
    "amqp",
    "message broker"
  ],
  "license": "Apache-2.0",
  "author": "Gajewski Dmitriy Yurevish <d.y.gaevskiy@tinkoff.ru>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "repository": "https://github.com/Tinkoff/mbclient.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "eslint src --ext .ts --max-warnings 0",
    "check": "npm run lint && npm run test:unit",
    "test": "npm run test:unit",
    "test:unit": "jest --coverage --verbose",
    "test:ci": "npm run test:unit -- --coverageReporters=lcov",
    "test:unit:watch": "jest --watch",
    "prepublishOnly": "npm run check && npm run build",
    "semantic-release": "semantic-release"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/amqplib": "^0.8.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^27.5.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
